{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "regions": ["iad1"],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.prairiesignal.ca" }],
      "destination": "https://prairiesignal.ca/$1",
      "permanent": true
    }
  ]
}
